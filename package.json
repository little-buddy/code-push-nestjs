{
  "name": "code-push-nestjs",
  "version": "5.7.1",
  "description": "Refactor code with nestjs based on code-push-server@5.7.1",
  "author": "buddy <litoubuddy@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "yarn run typeorm migration:generate -d ormconfig",
    "migration:create": "yarn run typeorm migration:create -d ormconfig",
    "new": "hygen new",
    "migration:revert": "yarn run typeorm migration:revert",
    "schema:drop": "yarn run typeorm schema:drop",
    "prepare": "husky install",
    "release": "release-it"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/lib-storage": "^3.400.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.2.2",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.2",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "ali-oss": "^6.18.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cos-nodejs-sdk-v5": "^2.12.4",
    "express": "^4.18.2",
    "express-ctx": "^0.1.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "jschardet": "^3.0.0",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nestjs-i18n": "^10.3.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "qiniu": "^7.9.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "typeorm-transactional": "^0.4.1",
    "upyun": "^3.4.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.4.0",
    "@handfish/hygen": "^6.1.6",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/ali-oss": "^6.16.8",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.197",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.3.1",
    "@types/shelljs": "^0.8.12",
    "@types/supertest": "^2.0.12",
    "@types/upyun": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-canonical": "^4.9.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.0.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "release-it": "^16.1.5",
    "run-script-webpack-plugin": "^0.2.0",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.88.2",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "setupFilesAfterEnv": [
      "dotenv/config"
    ]
  }
}
